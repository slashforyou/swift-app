{
  "expo": {
    "name": "swift-app",
    "slug": "swiftapp",
    "version": "0.0.1",
    "orientation": "portrait",
    "scheme": "swiftapp",
    "userInterfaceStyle": "automatic",
    "entryPoint": "App.tsx",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.slash4u.swiftapp"
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "197a33fd-1b2f-4c2e-b51b-b3ff8f976a80"
      }
    },
    "plugins": [
      "expo-secure-store"
    ],
    "owner": "slash4u"
  }
}
