{
  "expo": {
    "name": "Cobbr",
    "slug": "swiftapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "cobbr",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/logo-512.png",
    "splash": {
      "image": "./assets/images/logo-512.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F8FC"
    },
    "entryPoint": "App.tsx",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/logo-512.png",
      "bundleIdentifier": "com.slash4u.cobbr",
      "buildNumber": "1",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "icon": "./assets/images/logo-512.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-432.png",
        "backgroundColor": "#F6F8FC"
      },
      "package": "com.slash4u.cobbr",
      "versionCode": 1,
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "APP_ENV": "development",
      "eas": {
        "projectId": "197a33fd-1b2f-4c2e-b51b-b3ff8f976a80"
      }
    },
    "plugins": [
      "expo-secure-store",
      "expo-font",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#F6F8FC",
          "image": "./assets/images/logo-512.png",
          "imageWidth": 120
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.slash4u.cobbr",
          "enableGooglePay": true
        }
      ],
      "expo-asset"
    ],
    "owner": "slash4u",
    "updates": {
      "url": "https://u.expo.dev/197a33fd-1b2f-4c2e-b51b-b3ff8f976a80"
    }
  }
}
